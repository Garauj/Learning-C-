#include <iostream>
#include <locale>
#include <stdlib.h>
using namespace std;
int main()
{
setlocale(LC_ALL, "ptb");
float lado1, lado2, lado3;
 cout <<"indorme lado1 ";
 cin>>lado1;
 cout <<"informe lado2";
 cin >>lado2;
 cout <<"informe lado 3";
 cin >>lado3;
if(lado1==lado2&&lado1==lado2)
 {
 cout<<"equilátero";
}
 else if(lado1==lado2||lado2==lado3||lado1==lado3)
 {
 cout<<"isósceles";
}
 else
{
  
 cout<<"escaleno"; 
}
}

